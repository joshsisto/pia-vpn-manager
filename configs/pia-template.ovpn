client
dev tun
proto udp
remote SERVER_HOSTNAME 1198
resolv-retry infinite
nobind
persist-key
persist-tun
cipher aes-128-cbc
auth sha1
tls-client
remote-cert-tls server
auth-user-pass AUTH_FILE
compress lz4
verb 1
reneg-sec 0
crl-verify crl.rsa.4096.pem
ca ca.rsa.4096.crt
disable-occ

# Route configuration to preserve SSH and handle DNS
script-security 2
up ROUTE_UP_SCRIPT
down ROUTE_DOWN_SCRIPT